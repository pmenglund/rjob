- form_for @execution do |f|
  = f.error_messages
  %p
    = f.label :job_id
    %br
    = f.text_field :job_id
  %p
    = f.label :host
    %br
    = f.text_field :host
  %p
    = f.label :result
    %br
    = f.text_field :result
  %p
    = f.label :stdout
    %br
    = f.text_area :stdout
  %p
    = f.label :stderr
    %br
    = f.text_area :stderr
  %p
    = f.submit
